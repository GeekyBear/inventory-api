{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/setup-e2e.ts"
  ],
  "testTimeout": 120000,
  "collectCoverageFrom": [
    "../src/**/*.(t|j)s",
    "!../src/**/*.interface.ts",
    "!../src/**/*.dto.ts",
    "!../src/**/*.schema.ts",
    "!../src/main.ts",
    "!../src/test/**/*",
    "!../src/**/*.spec.ts"
  ],
  "coverageDirectory": "../coverage-e2e",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ]
}